{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Attach USB Device to WSL",
      "type": "shell",
      "command": "cmd.exe",
      "args": [
        "/c",
        "\"C:\\workspace\\forward_JLink_to_WSL.bat\""
      ],
      "presentation": {
        "panel": "shared",
        "echo": true,
        "reveal": "always"
      },
      "runOptions": {
        "runOn": "folderOpen"
      }
    },
    {
      "label": "Execute Startup Shell Script",
      "type": "shell",
      "command": "./startup.sh",
      "presentation": {
        "reveal": "always",
        "panel": "shared",
        "showReuseMessage": false
      },
      "runOptions": {
        "runOn": "folderOpen"
      }
    },
  ]
}
